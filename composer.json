{
    "name": "leorm037/speedtest",
    "description": "Gráfico com o teste de velocidade da Internet.",
    "authors": [
        {
            "name": "Leonardo Rodrigues Marques",
            "email": "leonardo@paginaemconstrucao.com.br",
            "homepage": "https://www.paginaemconstrucao.com.br",
            "role": "Developer"
        }
    ],
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "beberlei/doctrineextensions": "^1.5",
        "doctrine/doctrine-bundle": "^3.2.2",
        "doctrine/doctrine-migrations-bundle": "^4.0",
        "doctrine/orm": "^3.6.0",
        "phpdocumentor/reflection-docblock": "^5.6.6",
        "phpstan/phpdoc-parser": "^2.3",
        "symfony/asset": "8.0.*",
        "symfony/asset-mapper": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/expression-language": "8.0.*",
        "symfony/flex": "^2.10",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/mailer": "8.0.*",
        "symfony/messenger": "8.0.*",
        "symfony/mime": "8.0.*",
        "symfony/monolog-bundle": "^3.0|^4.0.1",
        "symfony/notifier": "8.0.*",
        "symfony/process": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/redis-messenger": "8.0.*",
        "symfony/requirements-checker": "^2.0.3",
        "symfony/runtime": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/stimulus-bundle": "^2.31",
        "symfony/string": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/uid": "8.0.*",
        "symfony/ux-icons": "^2.31",
        "symfony/ux-twig-component": "^2.31",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/yaml": "8.0.*",
        "twig/extra-bundle": "^2.12|^3.22.2",
        "twig/intl-extra": "^3.22.1",
        "twig/twig": "^2.12|^3.22.2"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*",
        "symfony/polyfill-php83": "*",
        "symfony/polyfill-php84": "*"
    },
    "scripts": {
        "pre-command-run": [
            "php bin/speedtest-output"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "requirements-checker": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "code:analyze": [
            "phpstan analyse --ansi"
        ],
        "code:fix": [
            "php-cs-fixer fix --ansi"
        ],
        "code:ci": [
            "@code:fix",
            "@code:analyze"
        ],
        "code:deploy": [
            "php bin/console ux:icon:warm-cache --ansi",
            "php bin/console importmap:update --ansi",
            "php bin/console asset-map:compile --ansi",
            "php bin/console cache:warmup --ansi",
            "php bin/console doctrine:migration:migrate --no-interaction --ansi"
        ]
    },
    "scripts-descriptions": {
        "code:ci": "Realiza analise do código fonte e testes automatizados."
    },
    "scripts-aliases": {
        "code:analyse": ["analyze", "analyse", "phpstan", "stan"]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*",
            "docker": false
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92.3",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpunit/phpunit": "^12.5.4",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    }
}
